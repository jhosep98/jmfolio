{
  "name": "jmfolio",
  "description": "JM Folio is a portfolio tracker application for managing investment positions across stocks, crypto, cash, and ETFs.",
  "version": "0.1.0",
  "private": true,
  "author": "@jhosep98",
  "license": "MIT",
  "keywords": [
    "portfolio",
    "investment",
    "tracker",
    "stocks",
    "crypto",
    "etf",
    "nextjs",
    "prisma"
  ],
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "repository": {
    "url": "https://github.com/jhosep98/jmfolio.git"
  },
  "packageManager": "bun@1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --fix .",
    "format": "biome format --write .",
    "check": "biome check --write",
