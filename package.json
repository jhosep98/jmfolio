    "negotiator": "1.0.0",
    "next": "16.0.1",
    "next-auth": "4.24.13",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.1",
